{
    "tools": {
        "curl": {
            "display": "cURL",
            "command": "curl -L \"alias-alchemy.ra101.dev?q=py,dj,docker&sys=linux\" > ~/.alias.sh",
            "link": "https://curl.se/"
        },
        "wget": {
            "display": "Wget",
            "command": "wget \"alias-alchemy.ra101.dev?q=py,dj,docker&sys=win\" > ~/.alias.cmd",
            "link": "https://www.gnu.org/software/wget/"
        },
        "httpie": {
            "display": "HTTPie",
            "command": "http -d [url] > [filepath]",
            "link": "https://httpie.io/"
        },
        "windowspowershell": {
            "display": "Invoke-WebRequest",
            "command": "Invoke-WebRequest [url] -O [filepath]",
            "link": "https://learn.microsoft.com/en-us/powershell/module/microsoft.powershell.utility/invoke-webrequest/"            
        },
        "aria2": {
            "display": "aria2",
            "command": "aria2c -c [url] > [filepath]",
            "link": "https://aria2.github.io/"
        },
        "postmanruntime": {
            "display": "Postman",
            "command": "GET [url] | Save to File",
            "link": "https://www.postman.com/"
        },
        "alal": {
            "display": "Any Tool",
            "command": "[tool] [header: {'User-Agent': 'alal'}] [url] > [filepath]",
            "link": null
        }
    },
    "aliasDetails": {
        "python": {
            "display": "Python",
            "shorthand": "py",
            "link": "https://www.python.org/"
        },
        "docker": {
            "display": "Docker",
            "shorthand": "dc",
            "link": "https://www.docker.com/"
        },
        "django": {
            "display": "Django",
            "shorthand": "dj",
            "link": "https://www.djangoproject.com/"
        }
    }
}